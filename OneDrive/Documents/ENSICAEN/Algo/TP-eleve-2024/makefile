all: clean exec

.PHONY: clean

clean:
	cd ./utils/ && $(MAKE) clean
	cd ./list/ && $(MAKE) clean
	cd ./queue/ && $(MAKE) clean
	cd ./stack/ && $(MAKE) clean
	cd ./heap/ && $(MAKE) clean
	cd ./hashtable/ && $(MAKE) clean
	cd ./bst/ && $(MAKE) clean
	cd ./rBST/ && $(MAKE) clean
	cd ./redBlackBST/ && $(MAKE) clean
	cd ./graph/ && $(MAKE) clean
	cd ./spanningtree/ && $(MAKE) clean
	cd ./dag/ && $(MAKE) clean


exec:
	cd ./utils/ && $(MAKE)
	cd ./list/ && $(MAKE)
	cd ./queue/ && $(MAKE)
	cd ./stack/ && $(MAKE)
	cd ./heap/ && $(MAKE)
	cd ./hashtable/ && $(MAKE)
	cd ./bst/ && $(MAKE)
	cd ./rBST/ && $(MAKE)
	cd ./redBlackBST/ && $(MAKE)
	cd ./graph/ && $(MAKE)
	cd ./spanningtree/ && $(MAKE)
	cd ./dag/ && $(MAKE)
